{
  "expo": {
    "name": "axed",
    "slug": "axed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nnagelia.axed",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/playfair-display/PlayfairDisplay_400Regular.ttf",
            "node_modules/@expo-google-fonts/playfair-display/PlayfairDisplay_500Medium.ttf",
            "node_modules/@expo-google-fonts/playfair-display/PlayfairDisplay_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/playfair-display/PlayfairDisplay_700Bold.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_400Regular.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_500Medium.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_700Bold.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_300Light.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "89112c2c-d4d1-418b-bec7-9b0a7aaf4dcd"
      }
    }
  }
}
